{
  "vim.disableAnnoyingNeovimMessage": true,
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "workbench.iconTheme": "vscode-icons",
  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "editor.parameterHints": false,
  "workbench.statusBar.visible": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#2e2e2e",
    "panel.border": "#494949",
    "sideBar.border": "#494949",
    "terminal.background": "#1c1d21",
    "terminal.foreground": "#839496",
    "terminal.ansiBlack": "#1c1d21",
    "terminal.ansiBlue": "#268bd2",
    "terminal.ansiBrightBlack": "#586e75",
    "terminal.ansiBrightBlue": "#19b6d6",
    "terminal.ansiBrightCyan": "#93a1a1",
    "terminal.ansiBrightGreen": "#66bd6c",
    "terminal.ansiBrightMagenta": "#6c6ec6",
    "terminal.ansiBrightRed": "#cb4b16",
    "terminal.ansiBrightWhite": "#fdf6e3",
    "terminal.ansiBrightYellow": "#eccd65",
    "terminal.ansiCyan": "#2aa198",
    "terminal.ansiGreen": "#66bd6c",
    "terminal.ansiMagenta": "#d33682",
    "terminal.ansiRed": "#dc322f",
    "terminal.ansiWhite": "#eee8d5",
    "terminal.ansiYellow": "#eccd65"
  },
  "editor.scrollBeyondLastLine": true,
  "workbench.colorTheme": "An Old Hope",
  "editor.fontSize": 14,
  "eslint.packageManager": "yarn",
  "explorer.confirmDragAndDrop": false,
  "window.zoomLevel": 0,
  "cSpell.language": "en",
  "workbench.startupEditor": "newUntitledFile",
  "cSpell.userWords": [
    "Parens",
    "browserslist",
    "chimper",
    "crossorigin",
    "destin",
    "destinio",
    "feathersjs",
    "linebreak",
    "phablet",
    "scrollbar",
    "servlet",
    "socketio"
  ],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "files.exclude": {
    "**/.git": true
  },
  "java.autobuild.enabled": false,
  "java.import.gradle.enabled": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "DroidSansMono Nerd Font",
  "editor.minimap.enabled": false,
  "emojisense.languages": {
    "markdown": true,
    "javascript": true,
    "plaintext": {
      "markupCompletionsEnabled": false,
      "emojiDecoratorsEnabled": false
    },
    "git-commit": true
  },
  "vsicons.dontShowNewVersionMessage": true,
  "java.configuration.checkProjectSettingsExclusions": false,
  "gitlens.hovers.currentLine.over": "line",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.fontLigatures": true,
  "editor.fontFamily": "FiraCode-Retina",
  "editor.fontWeight": "500",
  "typescript.format.semicolons": "insert",
  "editor.formatOnSave": true,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "prettier.jsxSingleQuote": true,
  "prettier.trailingComma": "all"
}
